<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title Eshop</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-2.1.3.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/angular-ui-router.js"></script>
    <script src="~/ScriptsApp/Eshop.js"></script>
    <script src="~/ScriptsApp/Controllers/AdminController.js"></script>
    <script src="~/ScriptsApp/configAdminas.js"></script>
    <script src="~/ScriptsApp/configVartotojas.js"></script>
    <script src="~/ScriptsApp/Controllers/KrepselisController.js"></script>
    <script src="~/ScriptsApp/Controllers/UzsakymaiController.js"></script>
    <base />
</head>
<body data-ng-app="EshopApp">
    <nav role="navigation" class="navbar navbar-default navbar-static-top">
        <div class="navbar-header">
            <button type="button" data-target="#navbarCollapse" data-toggle="collapse" class="navbar-toggle">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
        </div>
        <div id="navbarCollapse" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <li>@Html.ActionLink("Prekes", "Index", "Vartotojas")</li>
                <li>@Html.ActionLink("Adminas", "Index", "Admin")</li>
            </ul>
        </div>
    </nav>
    @RenderBody()
</body>
</html>
